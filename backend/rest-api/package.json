{
    "scripts": {
        "build:migrations": "tsc -p tsconfig-migrations.json",
        "migrate:latest": "node ./js-output/utils/migrate/migrate-latest.js",
        "migrate:previous": "node ./js-output/utils/migrate/migrate-previous.js",
        "migrate:next": "node ./js-output/utils/migrate/migrate-next.js",
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc -p tsconfig.json -w",
        "start:init": "unset name && pm2 start ./js-output/index.js -i 'max' --watch --name rest-api",
        "start": "pm2 start rest-api --watch",
        "stop": "pm2 stop rest-api",
        "gentypes": "kysely-codegen --verify --dialect postgres",
        "format": "prettier . --write",
        "test": "NODE_ENV='test' node --test ./js-output/modules/**/*-tests.js"
    },
    "dependencies": {
        "@fastify/autoload": "^5.8.0",
        "@fastify/cookie": "^9.3.1",
        "@fastify/cors": "^9.0.1",
        "@fastify/env": "^4.3.0",
        "@fastify/helmet": "^11.1.1",
        "@fastify/multipart": "^8.1.0",
        "@fastify/postgres": "^5.2.2",
        "@fastify/rate-limit": "^9.1.0",
        "@fastify/sensible": "^5.5.0",
        "@fastify/swagger": "^8.14.0",
        "@fastify/swagger-ui": "^3.0.0",
        "@fastify/type-provider-typebox": "^4.0.0",
        "@lucia-auth/adapter-postgresql": "^3.1.1",
        "@sinclair/typebox": "^0.32.15",
        "arctic": "^1.2.1",
        "fastify": "^4.26.2",
        "fastify-cli": "^6.1.1",
        "fastify-plugin": "^4.5.1",
        "kysely": "^0.27.3",
        "lucia": "^3.1.1",
        "pg": "^8.11.3",
        "pm2": "^5.3.1",
        "prettier": "^3.2.5"
    },
    "type": "module",
    "devDependencies": {
        "@tsconfig/node20": "^20.1.3",
        "@types/node": "^20.11.30",
        "@types/pg": "^8.11.4",
        "kysely-codegen": "^0.14.1",
        "pino-pretty": "^11.0.0",
        "typescript": "^5.4.2"
    }
}
